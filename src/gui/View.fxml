<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="234.0" prefWidth="242.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="gui.ViewController">
   <children>
      <Label layoutX="70.0" layoutY="35.0" text="Calculate Sum">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtNumber1" layoutX="51.0" layoutY="78.0" promptText="First Number" />
      <TextField fx:id="txtNumber2" layoutX="51.0" layoutY="125.0" promptText="Second Number" />
      <Button fx:id="btSum" layoutX="51.0" layoutY="166.0" mnemonicParsing="false" onAction="#onBtSumAction" text="Sum" />
      <Label layoutX="120.0" layoutY="170.0" text="Result = " />
      <Label fx:id="labelResult" layoutX="168.0" layoutY="170.0" />
   </children>
</AnchorPane>
